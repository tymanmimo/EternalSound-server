{
  "module": "./esm/mod.js",
  "main": "./script/mod.js",
  "types": "./types/mod.d.ts",
  "name": "libmuse",
  "version": "0.0.87",
  "description": "A library to interact with the YouTube Music (InnerTube) api.",
  "tags": [
    "youtube",
    "music",
    "api",
    "youtube-music",
    "innertube",
    "ytmusicapi",
    "muse"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vixalien/muse.git"
  },
  "bugs": {
    "url": "https://github.com/vixalien/muse/issues"
  },
  "files": [
    "/esm",
    "/script",
    "/types",
    "LICENSE",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./types/mod.d.ts",
        "default": "./esm/mod.js"
      },
      "require": {
        "types": "./types/mod.d.ts",
        "default": "./script/mod.js"
      }
    },
    "./auth.js": {
      "import": {
        "types": "./types/auth.d.ts",
        "default": "./esm/auth.js"
      },
      "require": {
        "types": "./types/auth.d.ts",
        "default": "./script/auth.js"
      }
    },
    "./request.js": {
      "import": {
        "types": "./types/request.d.ts",
        "default": "./esm/request.js"
      },
      "require": {
        "types": "./types/request.d.ts",
        "default": "./script/request.js"
      }
    },
    "./store.js": {
      "import": {
        "types": "./types/store.d.ts",
        "default": "./esm/store.js"
      },
      "require": {
        "types": "./types/store.d.ts",
        "default": "./script/store.js"
      }
    }
  },
  "dependencies": {
    "jsonpath-plus": "7.2.0"
  }
}